{
  "name": "@tonyfg/denormalize_json_api",
  "version": "0.1.0",
  "description": "Denormalize JSON API responses so the shape of returned objects is easier to access",
  "keywords": [
    "jsonapi",
    "json:api",
    "json",
    "api",
    "normalize",
    "denormalize",
    "normalizer",
    "denormalizer"
  ],
  "main": "index.js",
  "repository": "github:tonyfg/denormalize_json_api",
  "homepage": "https://github.com/tonyfg/denormalize_json_api",
  "bugs": "https://github.com/tonyfg/denormalize_json_api/issues",
  "author": "Tony Gon√ßalves <tonyfg.pt@gmail.com> (https://github.com/tonyfg)",
  "license": "BSD-1-Clause",
  "private": false,
  "files": [
    "index.js"
  ],
  "scripts": {
    "lint": "eslint . --max-warnings 0",
    "lint-fix": "eslint . --max-warnings 0 --fix",
    "pretty": "prettier -c ./*.js ./**/*.js",
    "pretty-fix": "prettier --write ./*.js ./**/*.js",
